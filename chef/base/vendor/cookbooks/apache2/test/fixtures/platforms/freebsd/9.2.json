{
    "apache": {
        "dir": "/usr/local/etc/apache22",
        "lib_dir": "/usr/local/libexec/apache22",
        "cache_dir": "/var/run/apache22",
        "conf": "/usr/local/etc/apache22/httpd.conf",
        "perl_pkg": "perl5",
        "log_dir": "/var/log",
        "root_group": "root",
        "service_name": "apache22",
        "default_modules": [
            "status",
            "alias",
            "auth_basic",
            "authn_file",
            "authz_default",
            "authz_groupfile",
            "authz_host",
            "authz_user",
            "autoindex",
            "dir",
            "env",
            "mime",
            "negotiation",
            "setenvif"
        ]
    }
}
